    "build:static:zh": "TARGET_LANG=zh node init-env.js && gatsby clean && gatsby build && node post-build.js",
    "build:static:id": "TARGET_LANG=id node init-env.js && gatsby clean && gatsby build && node post-build.js",
    "build:static:ru": "TARGET_LANG=ru node init-env.js && gatsby clean && gatsby build && node post-build.js",
    "develop": "npm run clean && gatsby develop",
    "serve": "node server.js",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
